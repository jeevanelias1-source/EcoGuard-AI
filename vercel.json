{
      "builds": [
              {
                        "src": "backend/main.py",
                        "use": "@vercel/python"
              }
      ],
      "routes": [
              {
                        "src": "/api/(.*)",
                        "dest": "backend/main.py"
              },
              {
                        "src": "/",
                        "dest": "frontend/index.html"
              },
              {
                        "src": "/(.*)",
                        "dest": "frontend/$1"
              }
      ]
}
